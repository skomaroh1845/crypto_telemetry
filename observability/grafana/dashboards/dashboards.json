{
  "id": null,
  "title": "Telemetry Overview",
  "tags": [
    "telemetry",
    "overview",
    "all-services"
  ],
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Request Rate (RPS)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(otel_data_service_request_count_total{route=~\"$route\"}[$__rate_interval])) by (route)",
          "legendFormat": "data-service",
          "refId": "A"
        },
        {
          "expr": "sum(rate(otel_notifier_requests_total[$__rate_interval]))",
          "legendFormat": "notifier-service",
          "refId": "B"
        },
        {
          "expr": "sum(rate(otel_decision_service_request_count_total{route=~\"$route\"}[$__rate_interval])) by (route)",
          "legendFormat": "decision-service",
          "refId": "C"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "min": 0,
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "fillOpacity": 10,
            "gradientMode": "none"
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Successfull requests rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "100 * (1 - clamp_max((sum(rate(otel_data_service_request_error_count_total{route=~\"$route\"}[$__rate_interval])) by (route) or vector(0)) / clamp_min((sum(rate(otel_data_service_request_count_total{route=~\"$route\"}[$__rate_interval])) by (route) or vector(0.0001)), 0.0001), 1))",
          "legendFormat": "data-service",
          "refId": "A"
        },
        {
          "expr": "100 * (1 - clamp_max((sum(rate(otel_notifier_errors_total[$__rate_interval])) or vector(0)) / clamp_min((sum(rate(otel_notifier_requests_total[$__rate_interval])) or vector(0.0001)), 0.0001), 1))",
          "legendFormat": "notifier-service",
          "refId": "B"
        },
        {
          "expr": "100 * (1 - clamp_max((sum(rate(otel_decision_service_request_error_count_total{route=~\"$route\"}[$__rate_interval])) by (route) or vector(0)) / clamp_min((sum(rate(otel_decision_service_request_count_total{route=~\"$route\"}[$__rate_interval])) by (route) or vector(0.0001)), 0.0001), 1))",
          "legendFormat": "decision-service",
          "refId": "C"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "color": {
            "mode": "palette-classic"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": 80
              },
              {
                "color": "yellow",
                "value": 90
              },
              {
                "color": "green",
                "value": 95
              }
            ]
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "fillOpacity": 10,
            "gradientMode": "none"
          }
        }
      }
    },
    {
      "id": 3,
      "title": "Latency (P50, P95, P99)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(otel_data_service_request_duration_sec_seconds_bucket{route=~\"$route\"}[$__rate_interval])) by (route, le))",
          "legendFormat": "data-service P95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(otel_notifier_processing_duration_seconds_bucket[$__rate_interval])) by (le))",
          "legendFormat": "notifier-service P95",
          "refId": "E"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(otel_decision_service_request_duration_sec_seconds_bucket{route=~\"$route\"}[$__rate_interval])) by (route, le))",
          "legendFormat": "decision-service P95",
          "refId": "H"
        }
      ],
      "gridPos": {
        "h": 12,
        "w": 24,
        "x": 0,
        "y": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0,
          "color": {
            "mode": "palette-classic"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 8
              },
              {
                "color": "red",
                "value": 10
              }
            ]
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "fillOpacity": 10,
            "gradientMode": "none"
          }
        }
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {},
  "templating": {
    "list": [
      {
        "name": "route",
        "type": "query",
        "query": "label_values(otel_data_service_request_count_total, route)",
        "current": {
          "selected": false,
          "text": "All",
          "value": "$__all"
        },
        "options": [
          {
            "selected": true,
            "text": "All",
            "value": "$__all"
          }
        ],
        "includeAll": true,
        "multi": true,
        "allValue": ".*"
      }
    ]
  },
  "refresh": "30s",
  "schemaVersion": 37,
  "version": 1
}
